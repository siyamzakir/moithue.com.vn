{
    "post_type": {
        "default": "property"
    },
    "ID": {
        "mapping": "{post_id}"
    },
    "post_status": {
        "default": "publish"
    },
    "post_author" : {
        "default" : 1
    },
    "filter": {
        "default": "raw"
    },
    "post_content": {
        "mapping": "{PublicRemarks}"
    },
    "post_excerpt": {
        "mapping": "{PropertyType} - {PropertySubType}"
    },
    "post_title": {
        "mapping": "{UnparsedAddress}"
    },
    "post_name": {
        "mapping": "{ListingKey}"
    }
}
